PREFIX qa: <http://www.wdaqua.eu/qa#>
PREFIX oa: <http://www.w3.org/ns/openannotation/core/>
PREFIX questionID: <http://195.90.200.248:8090/question/stored-question__text_>

SELECT *
FROM ?graphURI
WHERE {
    ?annotationId a qa:AnnotationOfQuestionTranslation .
    ?annotationId oa:hasTarget ?question ;
                  oa:hasBody ?translation ;
                  oa:annotatedBy ?createdBy ;
                  oa:annotatedAt ?createdAt .
}