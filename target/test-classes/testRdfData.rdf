<?xml version="1.0" encoding="UTF-8"?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:ex="http://example.org/">

  <!-- Individual Resources -->
  <rdf:Description rdf:about="http://example.org/person/1">
    <ex:name>John Doe</ex:name>
    <ex:age rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">30</ex:age>
  </rdf:Description>

  <rdf:Description rdf:about="http://example.org/person/2">
    <ex:name>Jane Smith</ex:name>
    <ex:age rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">28</ex:age>
  </rdf:Description>

  <!-- Relationships -->
  <rdf:Description rdf:about="http://example.org/person/1">
    <ex:knows rdf:resource="http://example.org/person/2"/>
  </rdf:Description>

  <!-- More Data -->
  <rdf:Description rdf:about="http://example.org/place/1">
    <ex:placeName>New York City</ex:placeName>
  </rdf:Description>

</rdf:RDF>