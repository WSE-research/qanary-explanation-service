PREFIX urn: <http://fliqz.com/>
PREFIX oa: <http://www.w3.org/ns/openannotation/core/>
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX qa: <http://www.wdaqua.eu/qa#>
PREFIX dbpedia: <http://dbpedia.org/resource/>
PREFIX wikidata: <http://www.wikidata.org/entity/>
PREFIX annotationID: <tag:stardog:api:>
PREFIX questionID: <http://195.90.200.248:8090/question/stored-question__text_>
PREFIX owl: <http://www.w3.org/2002/07/owl#>

SELECT *
FROM ?graphURI
WHERE
{
	{
		SELECT ?id ?p ?o {
    	    ?id oa:annotatedBy ?componentURI.
    		?id rdf:type ?annotationType .
		    FILTER NOT EXISTS {?id rdf:type qa:AnnotationOfLogQuery}
	        ?id ?p ?o .
    	}
  	}
  	UNION {
	    SELECT (?o1 AS ?id) (?p2 AS ?p) (?o2 AS ?o) {
    		?id1 oa:annotatedBy ?componentURI .
    		?id1 rdf:type ?annotationType .
	  		FILTER NOT EXISTS {?id1 rdf:type qa:AnnotationOfLogQuery}
    		?id1 ?p1 ?o1 .
			?o1 ?p2 ?o2 .
  		}
  	}
  	UNION {
	    SELECT (?o2 AS ?id) (?p3 AS ?p) (?o3 AS ?o) {
    		?id1 oa:annotatedBy ?componentURI .
    		?id1 rdf:type ?annotationType .
            FILTER NOT EXISTS {?id1 rdf:type qa:AnnotationOfLogQuery}
      		?id1 ?p1 ?o1 .
			?o1 ?p2 ?o2 .
      		?o2 ?p3 ?o3
  		}
  	}
}